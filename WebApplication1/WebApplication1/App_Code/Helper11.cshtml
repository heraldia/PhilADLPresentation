@using System.Text


@helper timeConvert(string timeStr) { 
    @(timeStr.Substring(0,2) + ":" + timeStr.Substring(2,2) + ":" + timeStr.Substring(4, 2)  );
}
@helper dateConvert(string dateStr)
{
    @(dateStr.Substring(0, 4) + "-" + dateStr.Substring(4, 2) + "-" + dateStr.Substring(6, 2)  );
}


}
